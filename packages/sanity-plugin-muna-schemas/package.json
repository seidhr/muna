{
  "name": "@seidhr/sanity-plugin-muna-schemas",
  "version": "1.0.0-alpha.13",
  "description": "Muna or ᛗᚢᚾᚨ means remember in norse. Muna is a schema for the Sanity Studio that enables detailed descriptions of cultural heritage objects and knowledge about their contexts as well as pages about them (or anything else really.) ",
  "keywords": [
    "sanity",
    "sanity-plugin",
    "sanity-schema",
    "cidoc-crm",
    "cho"
  ],
  "homepage": "https://github.com/seidhr/muna#readme",
  "bugs": {
    "url": "https://github.com/seidhr/muna/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:seidhr/muna.git"
  },
  "license": "MIT",
  "author": "Tarje Sælen Lavik <tarje.lavik@gmail.com>",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "lint": "eslint ./src",
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "dev": "npm run link-watch",
    "watch": "pkg-utils watch --strict",
    "prepublishOnly": "run-s build",
    "create-index": "create-index -r ./src/schemas/properties",
    "create-sanity-json": "node ./scrips/createSanityJSON.mjs",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^2.10.12",
    "capitalize": "^2.0.4",
    "dayjs": "^1.10.7",
    "jsonata": "^1.8.5",
    "katex": "^0.15.1",
    "lodash": "^4.17.21",
    "prop-types": "15.7.2",
    "react-icons": "^4.11.0",
    "react-player": "^2.13.0",
    "sanity-plugin-hotspot-array": "^2.1.2"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^6.12.2",
    "@sanity/plugin-kit": "^4.0.19",
    "@types/react": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "create-index": "^2.6.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globby": "^12.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.1",
    "sanity": "^3.68.3",
    "styled-components": "^6.1.13",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "eslintConfig": {
    "parser": "sanipack/babel/eslint-parser",
    "extends": [
      "sanity",
      "sanity/react",
      "prettier",
      "prettier/react"
    ]
  }
}