{
  "name": "@seidhr/sanity-plugin-muna-schemas",
  "version": "1.0.0-alpha.13",
  "description": "Muna or ᛗᚢᚾᚨ means remember in norse. Muna is a schema for the Sanity Studio that enables detailed descriptions of cultural heritage objects and knowledge about their contexts as well as pages about them (or anything else really.) ",
  "keywords": [
    "sanity",
    "sanity-plugin",
    "sanity-schema",
    "cidoc-crm",
    "cho"
  ],
  "homepage": "https://github.com/seidhr/muna#readme",
  "bugs": {
    "url": "https://github.com/seidhr/muna/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:seidhr/muna.git"
  },
  "license": "MIT",
  "author": "Tarje Sælen Lavik <tarje.lavik@gmail.com>",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "lint": "eslint ./src",
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "dev": "npm run link-watch",
    "watch": "pkg-utils watch --strict",
    "prepublishOnly": "npm run build",
    "create-index": "create-index -r ./src/schemas/properties",
    "create-sanity-json": "node ./scrips/createSanityJSON.mjs",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^2.10.12",
    "capitalize": "^2.0.4",
    "dayjs": "^1.10.7",
    "react-icons": "^4.11.0"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^6.12.2",
    "@sanity/plugin-kit": "^4.0.19",
    "@types/react": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "create-index": "^2.6.0",
    "eslint": "^9",
    "typescript-eslint": "^8.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "globby": "^12.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "prettier-plugin-packagejson": "^2.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.1",
    "sanity": "^3.68.3",
    "styled-components": "^6.1.13",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=18"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "eslintImports": false
    }
  },
  "type": "module",
  "browserslist": "extends @sanity/browserslist-config",
  "sideEffects": false
}