{
  "name": "@seidhr/sanity-plugin-asset-source-marcus",
  "version": "1.0.0-alpha.0",
  "description": "Plugin for Sanity Studio providing a marcus.uib.no asset source. Get a thumbnail of the document and a link to the post on `asset.source.url`.",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "build": "sanipack build",
    "watch": "sanipack build --watch",
    "prepublishOnly": "sanipack build && sanipack verify"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:seidhr/muna.git"
  },
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "author": "Tarje Lavik <tarje.lavik@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "dependencies": {
    "@sanity/ui": "^0.33.4",
    "axios": "^0.19.2"
  },
  "devDependencies": {
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-sanity": "5.1.0",
    "eslint-plugin-react": "7.22.0",
    "prettier": "2.2.1",
    "sanipack": "1.0.8"
  },
  "homepage": "https://github.com/seidhr/muna#readme",
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "parser": "sanipack/babel/eslint-parser",
    "extends": [
      "sanity",
      "sanity/react",
      "prettier",
      "prettier/react"
    ]
  }
}
