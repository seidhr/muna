  "description": "Just links to Muna documentation, for now",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/seidhr/muna#readme",
  "bugs": {
    "url": "https://github.com/seidhr/muna/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:seidhr/muna.git"
  },
  "license": "MIT",
  "author": "Tarje Lavik <tarje.lavik@gmail.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "prepublishOnly": "npm run build",
    "dev": "npm run link-watch",
    "watch": "pkg-utils watch --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint ."
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/ui": "^3.1.11",
    "@seidhr/react-muna-logo": "*"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@sanity/pkg-utils": "^10.2.3",
    "@sanity/plugin-kit": "^4.0.20",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9",
    "typescript-eslint": "^8.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.20",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-is": "^19.2.3",
    "rimraf": "^6.0.0",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@sanity/dashboard": "^5.0.1",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "sanity": "^5.1.0",
    "styled-components": "^6.1.19"
  },
  "engines": {
    "node": ">=18"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "eslintImports": false
    }
  },
  "type": "module"
}